{
  "name": "klaro-empreinte",
  "version": "1.0.1+upstream.fce14a2",
  "description": "A simple but powerful consent manager. Empreinte digitale's fork.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empreinte-digitale/klaro.git"
  },
  "keywords": [
    "cookie",
    "consent",
    "gdpr",
    "compliance"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "cross-env APP_CSS=1 NODE_ENV=development webpack --watch",
    "cjs": "cross-env BABEL_ENV=cjs babel --out-dir lib src",
    "es": "cross-env BABEL_ENV=es babel --out-dir es src",
    "umd": "cross-env APP_CSS=1 BABEL_ENV=umd webpack",
    "umd-no-css": "cross-env BABEL_ENV=umd webpack",
    "build": "npm run clean-build && npm run cjs && npm run es && cross-env NODE_ENV=production npm run umd && cross-env NODE_ENV=production npm run umd-no-css",
    "clean-build": "rm -Rf ./dist/klaro*js && rm -Rf ./lib && rm -Rf ./es",
    "prepublishOnly": "npm run build"
  },
  "author": "kj7s GbR",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/empreinte-digitale/klaro/issues"
  },
  "homepage": "https://github.com/empreinte-digitale/klaro#readme",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "react-modal": "^3.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "file-loader": "^0.11.2",
    "json-loader": "^0.5.7",
    "node-sass": "^4.10.0",
    "preact": "^8.2.9",
    "preact-compat": "^3.18.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^0.5.9",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "yaml-loader": "^0.5.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16",
    "react-dom": "^0.14.0 || ^15.0.0 || ^16"
  }
}
